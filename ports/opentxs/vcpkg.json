{
    "name": "opentxs",
    "version": "1.205.0",
    "port-version": 0,
    "description":
        "The Open-Transactions project is a collaborative effort to develop a robust, commercial-grade, fully-featured, free-software toolkit implementing the OTX protocol as well as a full-strength financial cryptography library, API, CLI, and prototype server. The project is managed by a worldwide community of volunteers that use the Internet to communicate, plan, and develop the Open-Transactions toolkit and its related documentation.",
    "homepage": "https://opentransactions.org",
    "features": {
        "external-qt6": {
            "description": "Use Qt provided by system"
        },
        "matterfi": {
            "description": "Enable MatterFi features"
        },
        "pstl": {
            "description": "Use C++17 standard library parallel algorithms",
            "dependencies": [
                {
                    "name": "tbb",
                    "platform": "linux"
                }
            ]
        },
        "qt6": {
            "description": "Use Qt provided by vcpkg",
            "dependencies": [
                "qtbase",
                "qtdeclarative"
            ]
        },
        "test": {
            "description": "Build tests",
            "dependencies": [
                "gtest"
            ]
        },
        "tbb": {
            "description": "Use TBB for parallel algorithms",
            "dependencies": [
                "tbb"
            ]
        }
    },
    "default-features": [
        {
            "name": "pstl",
            "platform": "windows | linux"
        }
    ],
    "dependencies": [
        "boost-algorithm",
        {
            "name": "boost-asio",
            "features": [
                "ssl"
            ]
        },
        "boost-beast",
        "boost-circular-buffer",
        "boost-container",
        "boost-date-time",
        "boost-dynamic-bitset",
        "boost-endian",
        "boost-interprocess",
        "boost-iostreams",
        "boost-json",
        "boost-multiprecision",
        "boost-program-options",
        "boost-scope",
        "boost-smart-ptr",
        "boost-stacktrace",
        "boost-system",
        "boost-thread",
        "boost-type-index",
        "boost-unordered",
        "cmakerc",
        "libguarded",
        {
            "name": "libsodium",
            "version>=": "1.0.20#103"
        },
        "lmdb",
        {
            "name": "openssl",
            "version>=": "3.2.0"
        },
        {
            "name": "otcommon",
            "version>=": "3.1.2"
        },
        "protobuf",
        {
            "name": "pthread",
            "platform": "windows"
        },
        {
            "name": "pthread",
            "platform": "windows",
            "host": true
        },
        "secp256k1",
        "simpleini",
        "sqlite3",
        {
            "name": "vcpkg-cmake",
            "host": true
        },
        {
            "name": "vcpkg-cmake-config",
            "host": true
        },
        {
            "name": "zeromq",
            "features": [
                "curve",
                "sodium"
            ]
        },
        "zlib"
    ],
    "builtin-baseline": "8ccb84df727bdf83045e53c319af05c554838b80",
    "overrides": [
        {
            "name": "zeromq",
            "version": "2023-06-20#1"
        }
    ]
}
